<!DOCTYPE html>
<html lang="end">
    <head>
        <%- include('../partials/head.ejs'); %> 
    </head>
    <body>
        <header>
            <%- include('../partials/header.ejs'); %>
        </header>

        <main >
        <form method="POST" action="/add">
            <h1>Add Candidate</h1>
        
            <div>
            <label>Candidate Id:</label>
            <input type="text" name="studentId" placeholder="Enter you ID"/>
            </div>
            <div>
            <label>Candidate Name:</label>
            <input type="text" name="studentName" placeholder="Enter you name"/>
            </div>
            <br>
            <div>
                <button type="submit" >Add Candidate</button>

                <label><%=message%></label>
            </div>
        </form>

        </main>
        <footer>
            <%- include('../partials/footer.ejs'); %>
        </footer>
    </body>
</html>